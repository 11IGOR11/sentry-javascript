{
  "name": "@sentry/tracing",
  "version": "7.0.0-alpha.0",
  "description": "Extensions for Sentry AM",
  "repository": "git://github.com/getsentry/sentry-javascript.git",
  "homepage": "https://github.com/getsentry/sentry-javascript/tree/master/packages/tracing",
  "author": "Sentry",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sentry/minimal": "7.0.0-alpha.0",
    "@sentry/scope": "7.0.0-alpha.0",
    "@sentry/transport-base": "7.0.0-alpha.0",
    "@sentry/types": "7.0.0-alpha.0",
    "@sentry/utils": "7.0.0-alpha.0"
  },
  "devDependencies": {
    "@sentry/browser": "7.0.0-alpha.0",
    "@types/express": "^4.17.1",
    "@types/jsdom": "^16.2.9"
  },
  "scripts": {
    "build": "yarn build:dev",
    "build:watch": "yarn build --watch",
    "build:dev": "rollup -c ../../rollup.config.dev.js",
    "build:prod": "rollup -c ../../rollup.config.prod.js",
    "clean": "rimraf cjs dist esm types coverage",
    "lint": "yarn lint:prettier & yarn lint:eslint",
    "lint:prettier": "prettier --write \"src/**\" \"test/**\"",
    "lint:eslint": "eslint --fix \"src/**\" \"test/**\"",
    "test": "jest -c ../../jest.config.js",
    "test:watch": "yarn test --watch",
    "TODO:": "Restore rollup bundle builds"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "sideEffects": [
    "./src/index.ts"
  ]
