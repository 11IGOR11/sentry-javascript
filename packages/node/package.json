    "@sentry/integration-node-globalhandlers": "7.0.0-alpha.0",
    "@sentry/integration-node-linkederrors": "7.0.0-alpha.0",
    "@sentry/minimal": "7.0.0-alpha.0",
    "@sentry/scope": "7.0.0-alpha.0",
    "@sentry/tracing": "7.0.0-alpha.0",
    "@sentry/transport-base": "7.0.0-alpha.0",
    "@sentry/transport-http": "7.0.0-alpha.0",
    "@sentry/types": "7.0.0-alpha.0",
    "@sentry/utils": "7.0.0-alpha.0",
