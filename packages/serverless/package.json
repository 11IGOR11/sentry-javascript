{
  "name": "@sentry/serverless",
  "version": "7.0.0-alpha.0",
  "description": "Offical Sentry SDK for various serverless solutions",
  "repository": "git://github.com/getsentry/sentry-javascript.git",
  "homepage": "https://github.com/getsentry/sentry-javascript/tree/master/packages/serverless",
  "author": "Sentry",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sentry/minimal": "7.0.0-alpha.0",
    "@sentry/node": "7.0.0-alpha.0",
    "@sentry/scope": "7.0.0-alpha.0",
    "@sentry/tracing": "7.0.0-alpha.0",
    "@sentry/types": "7.0.0-alpha.0",
    "@sentry/utils": "7.0.0-alpha.0",
    "@types/aws-lambda": "^8.10.62",
    "@types/express": "^4.17.2"
  },
  "devDependencies": {
    "@google-cloud/bigquery": "^5.3.0",
    "@google-cloud/common": "^3.4.1",
    "@google-cloud/functions-framework": "^1.7.1",
    "@google-cloud/pubsub": "^2.5.0",
    "aws-sdk": "^2.765.0",
    "find-up": "^5.0.0",
    "google-gax": "^2.9.0",
    "nock": "^13.0.4",
    "npm-packlist": "^2.1.4",
    "read-pkg": "^5.2.0"
  },
  "scripts": {
    "build": "yarn build:dev",
    "build:watch": "yarn build --watch",
    "build:dev": "rollup -c ../../rollup.config.dev.js",
    "build:prod": "rollup -c ../../rollup.config.prod.js",
    "clean": "rimraf cjs dist esm types coverage",
    "lint": "yarn lint:prettier & yarn lint:eslint",
    "lint:prettier": "prettier --write \"src/**\" \"test/**\"",
    "lint:eslint": "eslint --fix \"src/**\" \"test/**\"",
    "test": "jest -c ../../jest.config.js",
    "test:watch": "yarn test --watch",

